    <section class="welcome-page" id="aboutus">

        <div class="shares-welcome-page">
            <h1 class="header-shares-welcome-page">
                Отдых без переплат – выбор умных путешественников
            </h1>
            <div class="welcome-page-swiper-container">
                <div class="block-left-shares-welcome-page">
                    <button class="swiper-btn left">
                        <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" stroke="#000000"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier"> <path d="M5 12H19M5 12L11 6M5 12L11 18" stroke="#ffffff" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path> </g></svg>
                    </button>
                    <swiper-container effect="fade" loop="true">

                        <% tours.forEach(tour => { %>
                            <swiper-slide>
                                <h3 class="title"><%= tour.name %></h3>
                                <p><span>Начало:</span> <%= new Date(tour.startDate).toLocaleDateString() %></p>
                                <p><span>Конец:</span> <%= new Date(tour.endDate).toLocaleDateString() %></p>
                                <p><span>Город:</span> <%= tour.city %></p>
                                <p><span>Услуги:</span> <%= tour.services %></p>
                                <div class="price">
                                     <%= tour.price.toLocaleString('ru-RU', { style: 'currency', currency: 'RUB', maximumFractionDigits: 0 }) %>
                                </div>
                                <p class="amount">Осталось <%= tour.amount %></p>
                                <button class="buy_button" value="<%= tour.id %>">ПОДАТЬ ЗАЯВКУ</button>
                            </swiper-slide>
                        <% }) %>
                    </swiper-container>
                    <button class="swiper-btn right">
                        <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" stroke="#000000"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier"> <path d="M5 12H19M5 12L11 6M5 12L11 18" stroke="#ffffff" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path> </g></svg>
                    </button>
                    <!-- <p class="description-shares-welcome-page">Лучшие предложения для отдыха</p>
